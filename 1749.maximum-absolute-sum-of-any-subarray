leetcode:https://leetcode.com/problems/maximum-absolute-sum-of-any-subarray/?envType=daily-question&envId=2025-02-26
java
class Solution {
    public int maxAbsoluteSum(int[] nums) {
        int max=nums[0];
        int sum=nums[0];
        for(int i=1;i<nums.length;i++){
           
          //  for(int j=i;j<nums.length;j++){
             sum = Math.max(sum + nums[i], nums[i]);
               // sum= (sum+nums[j]);
            max=Math.max(max,Math.abs(sum));
            }
       // }
        return max;

    }
}
